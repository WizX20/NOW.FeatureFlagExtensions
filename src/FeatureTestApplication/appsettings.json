{
  "AllowedHosts": "*",
  "ApiVersioning": {
    "DefaultVersion": {
      "Major": 1,
      "Minor": 0
    },
    "ApiDescription": {
      "Title": "Feature Test API",
      "Description": "",
      "Contact": {
        "Name": "NOW",
        "Email": "nerdsonwaves@outlook.com"
      },
      "License": {
        "Name": "MIT",
        "Url": "https://mit-license.org/"
      }
    }
  },
  "FeatureManagement": {
    "TestServiceTwo": {
      "EnabledFor": [
        {
          "Name": "Aggregate",
          "Parameters": {
            "Environments": [ "Local", "Development" ],
            "RequiredHeaders": [ "TestServiceTwo" ]
          }
        }
      ]
    },
    "TestServiceThree": {
      "EnabledFor": [
        {
          "Name": "Aggregate",
          "Parameters": {
            "Environments": [ "Local", "Development" ],
            "RequiredHeaders": [ "TestServiceThree" ]
          }
        }
      ]
    },
    "TestConfigurationTwo": {
      "EnabledFor": [
        {
          "Name": "Aggregate",
          "Parameters": {
            "Environments": [ "Local", "Development" ],
            "RequiredHeaders": [ "TestConfigurationTwo" ]
          }
        }
      ]
    },
    "TestConfigurationThree": {
      "EnabledFor": [
        {
          "Name": "Aggregate",
          "Parameters": {
            "Environments": [ "Local", "Development" ],
            "RequiredHeaders": [ "TestConfigurationThree" ]
          }
        }
      ]
    }
  },
  "FeatureTestApplication": {
    "CorsPolicy": {
      "AllowedOrigins": "*"
    },
    "Localization": {
      "DefaultCulture": "en-US",
      "SupportedCultures": "en-US"
    },
    "ResponseCaching": {
      "MaximumBodySize": 2048,
      "UseCaseSensitivePaths": false
    },
    "Swagger": {
      "JsonRoute": "swagger/{documentName}/swagger.json",
      "Description": "Feature Test API",
      "UIPrefix": ""
    }
  },
  "FeatureFlagExtensions": {
    "WebApi": {
      "DefaultVersion": {
        "Major": 1,
        "Minor": 0
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}